{
  "name": "crm.backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "db:create:dev": "cross-env NODE_ENV=development node scripts/db/create",
    "db:create:test": "cross-env NODE_ENV=test node scripts/db/create",
    "db:delete:dev": "cross-env NODE_ENV=development node scripts/db/delete",
    "db:delete:test": "cross-env NODE_ENV=test node scripts/db/delete",
    "db:flush:dev": "cross-env NODE_ENV=development npm run db:delete:dev && npm run db:create:dev",
    "db:flush:test": "cross-env NODE_ENV=test npm run db:delete:test && npm run db:create:test",
    "migrate:dev": "npm run build && cross-env NODE_ENV=development dotenv -e .env sequelize db:migrate && cross-env NODE_ENV=development dotenv -e .env sequelize db:seed:all",
    "migrate:test": "cross-env NODE_ENV=development sequelize db:migrate && cross-env NODE_ENV=development sequelize db:seed:all",
    "migrate:prod": "sequelize db:migrate --env production",
    "db:seed": "cross-env NODE_ENV=development sequelize db:seed:all",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start:debug": "cross-env NODE_ENV=development nodemon --inspect ./src/index.ts",
    "prestart": "npm run migrate:prod",
    "start": "node dist/index",
    "test": "jest",
    "test:dev": "jest --watchAll",
    "testWithCoverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build",
      "pre-push": "npm run build"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^12.20.16",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "concurrently": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "mocha": "^8.2.1",
    "nodemon": "^1.19.1",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "run-script-os": "^1.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@google-cloud/recommender": "^5.0.4",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/xml2js": "^0.4.11",
    "async-lock": "^1.4.0",
    "await-lock": "^2.2.2",
    "axios": "^0.21.0",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dateformat": "^4.4.1",
    "dotenv": "^8.2.0",
    "easy-soap-request": "^3.5.0",
    "eslint-plugin-jest": "^24.1.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "googleapis": "^110.0.0",
    "helmet": "^4.2.0",
    "jest": "^26.5.3",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lock": "^1.1.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "node-wsdl": "^1.0.1",
    "pdf-lib": "^1.17.1",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "soap": "^0.35.0",
    "strong-soap": "^3.0.0",
    "ts-jest": "^26.4.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23"
  }
}
